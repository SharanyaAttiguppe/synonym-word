<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SynonymSearchApp</display-name>
  <welcome-file-list>
    <welcome-file>View/WordSearch.jsp</welcome-file>
   
  </welcome-file-list>
  
   
  <servlet>
	  <servlet-name>WordSynonym</servlet-name>
	  <servlet-class>com.AlgoFocus.SynonymAPP.Controller.WordSearchController</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>WordSynonym</servlet-name>
	  <url-pattern>/wordSearch</url-pattern>
  </servlet-mapping>
  
<servlet>
	  <servlet-name>SaveData</servlet-name>
	  <servlet-class>com.AlgoFocus.SynonymAPP.Controller.InsertSynonymsWord</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>SaveData</servlet-name>
	  <url-pattern>/SaveData</url-pattern>
  </servlet-mapping>
</web-app>